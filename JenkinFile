pipeline{
    	agent any
    	tools {
  	maven 'm1'
	     }
	stages {
  	
  		stage('build') {
    				steps {
      					sh 'mvn clean install'
   					 }
  				}

  		stage('test') {
    steps {
      sh 'mvn test'
    }
  }


  stage('post') {
    steps {
     
      junit 'Amazon-Core/target/surefire-reports/*.xml'
    }
  }

}

}
